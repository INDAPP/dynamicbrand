<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.fdc.dynamicbrand">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GuideActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/activity_guide"
            />
        <activity android:name=".GenerationActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/activity_generation"/>
        <activity android:name=".BrandsActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/activity_brands"/>
        <activity android:name=".DescriptionActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/activity_description"/>
        <activity
            android:name=".InfoActivity"
            android:parentActivityName=".BrandsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LicencesActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/activity_info"/>


    </application>

</manifest>